        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage", 
                "corejs": 3, 
                "targets": {
                    "ie": "11"
                }
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-shorthand-properties",
        "@babel/plugin-proposal-object-rest-spread"
    ]
}